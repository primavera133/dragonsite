{
  "name": "dragonfly-guide",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "npx gatsby build",
    "dev": "npx gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "npx gatsby serve",
    "clean": "npx gatsby clean",
    "test": "npm run lockfile-lint",
    "lockfile-lint": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm"
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "cloudinary-react": "^1.2.1",
    "dotenv": "^8.2.0",
    "gatsby": "^2.18.12",
    "gatsby-plugin-emotion": "^4.1.16",
    "gatsby-plugin-google-analytics": "^2.1.31",
    "gatsby-plugin-manifest": "^2.2.34",
    "gatsby-plugin-offline": "^3.0.32",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-typography": "^2.3.18",
    "gatsby-source-graphql": "^2.1.25",
    "graphql": "^14.5.8",
    "react": "^16.12.0",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-images": "^1.0.0",
    "react-showdown": "^1.6.0",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-lincoln": "^0.16.19"
  },
  "devDependencies": {
    "lockfile-lint": "^3.0.4",
    "prettier": "^1.19.1"
  }
}
